{
  "name": "@arianrhodsandlot/eslint-config",
  "version": "0.15.11",
  "description": "A set of predefined ESLint config.",
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "license": "MIT",
  "author": "arianrhodsandlot",
  "type": "module",
  "packageManager": "pnpm@9.7.1",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/umd/index.cjs",
    "types": "./dist/types/index.d.ts"
  },
  "main": "dist/umd/index.cjs",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "conc npm:build:*",
    "build:esm": "swc src -d dist/esm --strip-leading-paths",
    "build:types": "tsc",
    "build:umd": "swc src -d dist/umd --strip-leading-paths -C module.type=umd --out-file-extension=cjs && node --experimental-strip-types scripts/fix-cjs-require.ts",
    "dev": "conc npm:dev:*",
    "dev:build": "pnpm build:esm -w",
    "dev:inspect": "eslint-config-inspector",
    "lint": "eslint --flag unstable_ts_config src",
    "test": "node --experimental-strip-types --test tests/*.test.ts"
  },
  "dependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.0",
    "@eslint/compat": "^1.1.1",
    "@eslint/js": "^9.9.0",
    "@intlify/eslint-plugin-vue-i18n": "3.0.0",
    "@next/eslint-plugin-next": "^14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-compat": "^6.0.0",
    "eslint-plugin-diff": "^2.0.3",
    "eslint-plugin-es-x": "^8.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^50.2.2",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-n": "^17.10.2",
    "eslint-plugin-perfectionist": "^3.2.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-plugin-regexp": "^2.6.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^1.0.4",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "eslint-plugin-tsdoc": "^0.3.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "eslint-plugin-vue": "^9.27.0",
    "eslint-plugin-vue-scoped-css": "^2.8.1",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "get-tsconfig": "^4.7.6",
    "globals": "^15.9.0",
    "lodash": "^4.17.21",
    "new-find-package-json": "^2.0.0",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.1.0",
    "vue-eslint-parser": "^9.4.3"
  },
  "devDependencies": {
    "@eslint/config-inspector": "0.5.3",
    "@swc/cli": "0.4.0",
    "@swc/core": "1.7.12",
    "@types/eslint": "9.6.0",
    "@types/eslint-config-prettier": "6.11.3",
    "@types/eslint-plugin-markdown": "2.0.2",
    "@types/eslint__js": "8.42.3",
    "@types/lodash": "4.17.7",
    "@types/node": "22.4.1",
    "concurrently": "8.2.2",
    "eslint": "9.9.0",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.6",
    "vue": "3.4.38",
    "vue-i18n": "9.14.0"
  },
  "peerDependencies": {
    "eslint": "*"
  },
  "peerDependenciesMeta": {
    "eslint": {
      "optional": false
    }
  }
}
